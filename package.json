{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "jest --runInBand"
  },
  "author": "Tushar Nimje",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nimjetushar/express-es6-rest-api.git"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "helmet": "3.16.0",
    "express": "4.16.4",
    "morgan": "1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/preset-env": "7.3.4",
    "babel-eslint": "10.0.1",
    "eslint": "5.8.0",
    "jest": "24.5.0",
    "nodemon": "1.9.2",
    "supertest": "4.0.2"
  },
  "jest": {
    "verbose": true
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}