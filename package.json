{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "jest --runInBand"
  },
  "author": "Tushar Nimje",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.13.3",
    "compression": "1.5.2",
    "cors": "2.7.1",
    "express": "4.13.3",
    "helmet": "3.15.0",
    "morgan": "1.8.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "eslint": "5.8.0",
    "jest": "^24.5.0",
    "nodemon": "1.9.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "verbose": true
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
