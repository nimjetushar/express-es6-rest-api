{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-2\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-2",
    "start": "node dist",
    "prestart": "npm run -s build",
    "lint": "eslint src"
  },
  "author": "Tushar Nimje",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.13.3",
    "compression": "1.5.2",
    "cors": "2.7.1",
    "express": "4.13.3",
    "helmet": "3.15.0",
    "morgan": "1.8.0"
  },
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-core": "6.9.0",
    "babel-eslint": "10.0.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-2": "6.24.1",
    "eslint": "5.8.0",
    "nodemon": "1.18.10"
  }
}
